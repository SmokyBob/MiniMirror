{
  "manifest_version": 2,
  "name": "Mini Mirror Edge",
  "version": "4",
  "minimum_chrome_version": "41",
  "icons": {
    "16": "icons/videocam_Edge16.png",
    "32": "icons/videocam_Edge32.png",
    "48": "icons/videocam_Edge48.png",
    "128": "icons/videocam_Edge128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "videoCapture",
    "app.window.alpha",
    "alwaysOnTopWindows",
    "storage"
  ]
}
