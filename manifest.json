{
  "manifest_version": 2,
  "name": "Mini Mirror",
  "version": "4",
  "minimum_chrome_version": "28",
  "icons": {
    "16": "icons/videocam_16.png",
    "32": "icons/videocam_32.png",
    "48": "icons/videocam_48.png",
    "128": "icons/videocam_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": [
    "videoCapture",
    "app.window.alpha",
    "alwaysOnTopWindows",
    "storage"
  ]
}
